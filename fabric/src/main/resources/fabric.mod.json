{
	"schemaVersion": 1,
	"id": "${modId}",
	"version": "${modVersion}",
	"name": "${modName}",
	"description": "${modDescription}",
	"authors": [
		"${modAuthor}"
	],
	"contact": {
		"homepage": "${modUrl}",
		"sources": "${modUrl}"
	},
	"license": "MIT",
	"icon": "logo.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"com.github.exopandora.shouldersurfing.fabric.ShoulderSurfingFabric"
		],
		"jade": [
			"com.github.exopandora.shouldersurfing.integration.ShoulderSurfingJadePlugin"
		]
	},
	"mixins": [
		"shouldersurfing.common.mixins.json",
		"shouldersurfing.common.compat.mixins.json",
		"shouldersurfing.fabric.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14",
		"fabric": ">=0.49.0",
		"minecraft": "~1.19",
		"forgeconfigapiport": ">=4.0.0",
		"java": ">=17"
	},
	"breaks": {
		"betterthirdperson": "*",
		"cameraoverhaul": "*",
		"nimble": "*",
		"valkyrienskies": "*"
	},
	"conflicts": {
		"theoneprobe": "*"
	},
	"custom": {
		"waila:plugins": {
			"id": "shouldersurfing:waila_plugin",
			"initializer": "com.github.exopandora.shouldersurfing.integration.ShoulderSurfingWthitPlugin",
			"environment": "client"
		}
	}
}
