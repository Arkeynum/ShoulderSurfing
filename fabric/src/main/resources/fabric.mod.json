{
	"schemaVersion": 1,
	"id": "shouldersurfing",
	"version": "${version}",
	"name": "Shoulder Surfing Reloaded",
	"description": "Shoulder Surfing Reloaded is a highly configurable third person camera mod for minecraft, originally made by Sabar.",
	"authors": [
		"Exopandora"
	],
	"contact": {
		"homepage": "https://www.curseforge.com/minecraft/mc-mods/shoulder-surfing-reloaded",
		"sources": "https://github.com/Exopandora/shouldersurfing"
	},
	"license": "MIT",
	"icon": "logo.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"com.github.exopandora.shouldersurfing.fabric.ShoulderSurfingFabric"
		],
		"jade": [
			"com.github.exopandora.shouldersurfing.compatibility.ShoulderSurfingJadePlugin"
		]
	},
	"mixins": [
		"shouldersurfing.common.mixins.json",
		"shouldersurfing.fabric.mixins.json",
		"shouldersurfing.compat.iris.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14",
		"fabric": ">=0.83.0",
		"minecraft": ">=1.20.0 <=1.20.1",
		"forgeconfigapiport": ">=7.0.0",
		"java": ">=17"
	},
	"breaks": {
		"betterthirdperson": "*",
		"cameraoverhaul": "*",
		"nimble": "*"
	}
}
